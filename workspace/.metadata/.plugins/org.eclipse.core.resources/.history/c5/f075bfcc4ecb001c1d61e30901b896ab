'use strict';
// 1~45 사이에 중복되지 않게 6개의 숫자를 출력하시오

let luckyNumber = [];
let randomNum = [];
for(let i=1; i<=45; i++) {
    luckyNumber.push(i);
}

console.log(`${luckyNumber.toString()}`);

for(let i=1; i<=6; i++) {
    let idx = Math.floor(Math.random() * luckyNumber.length);
    
    for(let j=1;j<i-1;i++) {
        if(randomNum[j] == idx) {
            continue;
        }
    }
    
    randomNum.push(i);
    
}

console.log();