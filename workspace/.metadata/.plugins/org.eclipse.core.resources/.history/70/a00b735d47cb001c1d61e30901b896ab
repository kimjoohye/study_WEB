'use strict';

// forEach()
let numArray = [10,20,30,40,50,60,70];
let sum = 0;

numArray.forEach(function(value,index,array){
    console.log(`value : ${value}`);
    // console.log(`index : ${index}`);
    // console.log(`index : ${index}, ${array}`);
    sum+=value;
});
console.log(`sum=${sum}`); // sum=280