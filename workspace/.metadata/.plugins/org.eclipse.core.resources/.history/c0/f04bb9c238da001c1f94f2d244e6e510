	<h2>jstl:forEach(array)</h2>
	<hr/>
	<!-- int 타입의 배열인 intArray변수 생성 -->
  <c:set var="intArray" value="<%=new int[]{1,2,3,4,5} %>" />
  <c:forEach var="i" items="${intArray}">
  
  </c:forEach>
